{
  "name": "@aibos/ui-components-enterprise",
  "version": "1.0.0",
  "type": "module",
  "description": "AI-BOS Enterprise UI Components - ISO27001, GDPR, SOC2, HIPAA compliant React components with purpose-driven architecture",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.esm.js",
      "require": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./primitives": {
      "import": "./dist/primitives/index.esm.js",
      "require": "./dist/primitives/index.js",
      "types": "./dist/primitives/index.d.ts"
    },
    "./data": {
      "import": "./dist/data/index.esm.js",
      "require": "./dist/data/index.js",
      "types": "./dist/data/index.d.ts"
    },
    "./forms": {
      "import": "./dist/forms/index.esm.js",
      "require": "./dist/forms/index.js",
      "types": "./dist/forms/index.d.ts"
    },
    "./feedback": {
      "import": "./dist/feedback/index.esm.js",
      "require": "./dist/feedback/index.js",
      "types": "./dist/feedback/index.d.ts"
    },
    "./layout": {
      "import": "./dist/layout/index.esm.js",
      "require": "./dist/layout/index.js",
      "types": "./dist/layout/index.d.ts"
    },
    "./ai": {
      "import": "./dist/ai/index.esm.js",
      "require": "./dist/ai/index.js",
      "types": "./dist/ai/index.d.ts"
    },
    "./compliance": {
      "import": "./dist/compliance/index.esm.js",
      "require": "./dist/compliance/index.js",
      "types": "./dist/compliance/index.d.ts"
    },
    "./security": {
      "import": "./dist/security/index.esm.js",
      "require": "./dist/security/index.js",
      "types": "./dist/security/index.d.ts"
    },
    "./performance": {
      "import": "./dist/performance/index.esm.js",
      "require": "./dist/performance/index.js",
      "types": "./dist/performance/index.d.ts"
    },
    "./accessibility": {
      "import": "./dist/accessibility/index.esm.js",
      "require": "./dist/accessibility/index.js",
      "types": "./dist/accessibility/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run test && npm run build",
    "compliance:check": "node scripts/compliance-check.mjs",
    "security:audit": "node scripts/security-audit.mjs",
    "performance:benchmark": "node scripts/performance-benchmark.mjs",
    "accessibility:test": "node scripts/accessibility-test.mjs"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",

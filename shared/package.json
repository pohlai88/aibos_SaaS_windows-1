{
  "name": "@aibos/shared",
  "version": "1.0.0",
  "description": "AI-BOS Platform Shared Library - Enterprise-grade utilities for billing, caching, database, and performance optimization",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.esm.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./lib": {
      "import": "./dist/lib/index.esm.js",
      "require": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.esm.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./validation": {
      "import": "./dist/validation/index.esm.js",
      "require": "./dist/validation/index.js",
      "types": "./dist/validation/index.d.ts"
    },
    "./metadata": {
      "import": "./dist/types/metadata/index.esm.js",
      "require": "./dist/types/metadata/index.js",
      "types": "./dist/types/metadata/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.esm.js",
      "require": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./vscode": {
      "import": "./dist/vscode-extension/index.esm.js",
      "require": "./dist/vscode-extension/index.js",
      "types": "./dist/vscode-extension/index.d.ts"
    },
    "./docs": {
      "import": "./dist/docs/index.esm.js",
      "require": "./dist/docs/index.js",
      "types": "./dist/docs/index.d.ts"
    },
    "./monitoring": {
      "import": "./dist/monitoring/index.esm.js",
      "require": "./dist/monitoring/index.js",
      "types": "./dist/monitoring/index.d.ts"
    },
    "./debugging": {
      "import": "./dist/debugging/index.esm.js",
      "require": "./dist/debugging/index.js",
      "types": "./dist/debugging/index.d.ts"
    },
    "./dev-experience": {
      "import": "./dist/dev-experience/index.esm.js",
      "require": "./dist/dev-experience/index.js",
      "types": "./dist/dev-experience/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:compile && npm run build:optimize",
    "build:clean": "rimraf dist",
    "build:compile": "tsc",
    "build:optimize": "npm run build:esm && npm run build:analyze",
    "build:esm": "rollup -c rollup.config.js",
    "build:analyze": "npm run analyze:bundle",
    "analyze:bundle": "npx webpack-bundle-analyzer dist/stats.json",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:performance": "jest --testNamePattern='performance'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build && npm run test",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "benchmark": "ts-node scripts/benchmark.ts"
  },
  "keywords": [
    "aibos",
    "saas",
    "billing",
    "caching",
    "database",
    "queue",
    "performance",
    "enterprise",
    "typescript",
    "metadata",
    "validation"
  ],
  "author": "AI-BOS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pohlai88/aibos_SaaS_windows-1.git",
    "directory": "shared"
  },
  "bugs": {
    "url": "https://github.com/pohlai88/aibos_SaaS_windows-1/issues"
  },
  "homepage": "https://github.com/pohlai88/aibos_SaaS_windows-1#readme",
  "dependencies": {
    "zod": "^3.22.4",
    "redis": "^4.6.10",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "webpack-bundle-analyzer": "^4.10.1",
    "ts-node": "^10.9.2"
  },
  "peerDependencies": {
    "express": "^4.18.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
} 
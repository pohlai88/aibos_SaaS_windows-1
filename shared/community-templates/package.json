{
  "name": "@aibos/community-templates",
  "version": "1.0.0",
  "description": "AI-BOS Community Templates - Marketplace for sharing and discovering app templates with AI-powered recommendations",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./marketplace": {
      "types": "./dist/marketplace/index.d.ts",
      "import": "./dist/marketplace/index.mjs"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "import": "./dist/browser/index.mjs"
    },
    "./installer": {
      "types": "./dist/installer/index.d.ts",
      "import": "./dist/installer/index.mjs"
    },
    "./publisher": {
      "types": "./dist/publisher/index.d.ts",
      "import": "./dist/publisher/index.mjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint && npm run type-check",
    "build": "npm run build:clean && npm run build:compile && npm run build:optimize",
    "build:clean": "rimraf dist",
    "build:compile": "tsc",
    "build:optimize": "rollup -c rollup.config.js",
    "build:analyze": "rollup -c rollup.config.js --analyze",
    "build:storybook": "storybook build -o storybook-static",
    "dev": "tsc --watch",
    "dev:storybook": "storybook dev -p 6006",
    "type-check": "tsc --noEmit --incremental",
    "type-check:watch": "tsc --watch --noEmit --incremental",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "lint:a11y": "eslint . --max-warnings=0 --config .eslintrc.a11y.js",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "size-check": "bundlesize",
    "validate-templates": "node scripts/validate-templates.js",
    "docs:generate": "typedoc --out docs src",
    "docs:serve": "serve docs",
    "ci:test": "npm run test -- --reporter=verbose --coverage",
    "ci:lint": "npm run lint -- --format=unix",
    "ci:type-check": "npm run type-check",
    "ci:build": "npm run build",
    "postinstall": "husky install"
  },
  "bundlesize": [
    {
      "path": "./dist/index.mjs",
      "maxSize": "600 kB"
    },
    {
      "path": "./dist/marketplace/index.mjs",
      "maxSize": "300 kB"
    },
    {
      "path": "./dist/browser/index.mjs",
      "maxSize": "250 kB"
    },
    {
      "path": "./dist/installer/index.mjs",
      "maxSize": "200 kB"
    },
    {
      "path": "./dist/publisher/index.mjs",
      "maxSize": "250 kB"
    }
  ],
  "keywords": [
    "aibos",
    "community-templates",
    "template-marketplace",
    "app-templates",
    "template-sharing",
    "ai-recommendations",
    "enterprise",
    "storybook",
    "accessibility",
    "template-validation"
  ],
  "author": "AI-BOS Team",
  "license": "MIT",
  "dependencies": {
    "@aibos/shared": "^1.0.0",
    "@aibos/visual-dev": "^1.0.0",
    "@aibos/template-validator": "^1.0.0",
    "@aibos/analytics": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.525.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.22.4",
    "fuse.js": "^7.1.0",
    "react-virtualized": "^9.22.5",
    "react-intersection-observer": "^9.5.3",
    "react-preview": "^2.3.0",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-virtualized": "^9.21.24",
    "@types/lodash-es": "^4.17.12",
    "@types/serve": "^14.2.0",
    "typescript": "^5.2.2",
    "rollup": "^4.6.0",
    "vitest": "^1.6.1",
    "eslint": "^8.51.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "@storybook/react": "^7.5.3",
    "@storybook/addon-a11y": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "bundlesize": "^0.18.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.0",
    "typedoc": "^0.25.0",
    "serve": "^14.2.0",
    "@playwright/test": "^1.40.0",
    "axe-core": "^4.8.0",
    "@axe-core/react": "^4.8.0",
    "rollup-plugin-visualizer": "^5.10.0",
    "@types/react-dropzone": "^5.1.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@aibos/visual-dev": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,md,json}": [
      "prettier --write"
    ]
  }
}

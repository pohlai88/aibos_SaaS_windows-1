# ==================== AI-BOS PRODUCTION ENVIRONMENT ====================
# This file contains all production environment variables for AI-BOS

# ==================== APPLICATION CONFIGURATION ====================
NODE_ENV=production
PORT=3000
APP_NAME=AI-BOS
APP_VERSION=1.0.0
APP_ENV=production

# ==================== DATABASE CONFIGURATION ====================
# Supabase Configuration
SUPABASE_URL=https://your-supabase-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# PostgreSQL Configuration
DATABASE_URL=postgresql://username:password@host:port/database
DB_HOST=your-db-host
DB_PORT=5432
DB_NAME=ai_bos_production
DB_USER=ai_bos_user
DB_PASSWORD=your-secure-db-password

# ==================== AUTHENTICATION & SECURITY ====================
# JWT Configuration
JWT_SECRET=your-super-secure-jwt-secret-key-here
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# Session Configuration
SESSION_SECRET=your-super-secure-session-secret-key-here
SESSION_COOKIE_SECURE=true
SESSION_COOKIE_HTTPONLY=true
SESSION_COOKIE_SAMESITE=strict

# CORS Configuration
CORS_ORIGIN=https://ai-bos.vercel.app
CORS_CREDENTIALS=true

# ==================== AI & MACHINE LEARNING ====================
# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key
OPENAI_ORGANIZATION=your-openai-org-id

# Anthropic Configuration
ANTHROPIC_API_KEY=your-anthropic-api-key

# Ollama Configuration
OLLAMA_URL=http://localhost:11434
OLLAMA_MODELS=llama2,mistral,codellama

# ==================== MONITORING & LOGGING ====================
# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json
LOG_FILE=logs/ai-bos-production.log

# Monitoring Configuration
SENTRY_DSN=your-sentry-dsn
DATADOG_API_KEY=your-datadog-api-key
NEW_RELIC_LICENSE_KEY=your-newrelic-license-key

# ==================== EXTERNAL SERVICES ====================
# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-email-password
EMAIL_FROM=noreply@ai-bos.io

# File Storage Configuration
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=ai-bos-production

# Redis Configuration
REDIS_URL=redis://your-redis-host:6379
REDIS_PASSWORD=your-redis-password

# ==================== PERFORMANCE & CACHING ====================
# Cache Configuration
CACHE_TTL=3600
CACHE_MAX_SIZE=1000
CACHE_ENABLED=true

# Rate Limiting Configuration
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX_REQUESTS=100

# ==================== SECURITY & COMPLIANCE ====================
# Encryption Configuration
ENCRYPTION_KEY=your-32-character-encryption-key
ENCRYPTION_ALGORITHM=aes-256-gcm

# Compliance Configuration
GDPR_ENABLED=true
CCPA_ENABLED=true
HIPAA_ENABLED=false

# ==================== FEATURE FLAGS ====================
# Feature Toggles
FEATURE_AI_ENABLED=true
FEATURE_QUANTUM_ENABLED=false
FEATURE_BLOCKCHAIN_ENABLED=false
FEATURE_5G_ENABLED=false
FEATURE_EDGE_COMPUTING_ENABLED=false

# ==================== INTEGRATIONS ====================
# Third-party Integrations
SLACK_WEBHOOK_URL=your-slack-webhook-url
DISCORD_WEBHOOK_URL=your-discord-webhook-url
TELEGRAM_BOT_TOKEN=your-telegram-bot-token

# Payment Processing
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret
PAYPAL_CLIENT_ID=your-paypal-client-id
PAYPAL_CLIENT_SECRET=your-paypal-client-secret

# ==================== BACKUP & DISASTER RECOVERY ====================
# Backup Configuration
BACKUP_ENABLED=true
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30
BACKUP_S3_BUCKET=ai-bos-backups

# ==================== DEVELOPMENT & DEBUGGING ====================
# Debug Configuration (should be false in production)
DEBUG=false
DEBUG_LEVEL=error
ENABLE_PROFILING=false

# ==================== CUSTOM CONFIGURATION ====================
# Custom application settings
MAX_FILE_SIZE=10485760
SUPPORTED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf
MAX_CONCURRENT_UPLOADS=5

# ==================== API CONFIGURATION ====================
# API Rate Limits
API_RATE_LIMIT_GENERAL=1000
API_RATE_LIMIT_AUTH=100
API_RATE_LIMIT_AI=50
API_RATE_LIMIT_UPLOAD=10

# API Versioning
API_VERSION=v1
API_DEPRECATION_WARNING=true

# ==================== NOTIFICATIONS ====================
# Notification Configuration
NOTIFICATION_EMAIL_ENABLED=true
NOTIFICATION_SMS_ENABLED=false
NOTIFICATION_PUSH_ENABLED=true
NOTIFICATION_SLACK_ENABLED=true

# ==================== ANALYTICS ====================
# Analytics Configuration
GOOGLE_ANALYTICS_ID=your-ga4-id
MIXPANEL_TOKEN=your-mixpanel-token
AMPLITUDE_API_KEY=your-amplitude-api-key

# ==================== SUPPORT & MAINTENANCE ====================
# Support Configuration
SUPPORT_EMAIL=support@ai-bos.io
SUPPORT_PHONE=+1-555-0123
MAINTENANCE_MODE=false
MAINTENANCE_MESSAGE=System maintenance in progress. Please try again later.

# ==================== DEPLOYMENT ====================
# Deployment Configuration
DEPLOYMENT_ID=prod-$(date +%Y%m%d-%H%M%S)
DEPLOYMENT_ENVIRONMENT=production
DEPLOYMENT_REGION=us-east-1
DEPLOYMENT_VERSION=1.0.0

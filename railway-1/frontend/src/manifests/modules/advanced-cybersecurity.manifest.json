{
  "id": "advanced-cybersecurity",
  "version": "1.0.0",
  "type": "module",
  "enabled": true,
  "dependencies": ["core:app", "core:auth", "ui", "security"],
  "permissions": {
    "view": ["admin", "security_admin"],
    "monitor": ["admin", "security_admin"],
    "configure": ["security_admin"],
    "audit": ["admin", "security_admin"],
    "threat_detection": ["security_admin"],
    "incident_response": ["security_admin"],
    "vulnerability_scan": ["security_admin"],
    "penetration_test": ["security_admin"],
    "compliance_monitor": ["admin", "security_admin"],
    "access_control": ["security_admin"]
  },
  "config": {
    "defaults": {
      "features": {
        "threat_detection": true,
        "incident_response": true,
        "vulnerability_scanning": true,
        "penetration_testing": true,
        "compliance_monitoring": true,
        "access_control": true,
        "security_analytics": true,
        "real_time_monitoring": true,
        "automated_response": true,
        "security_orchestration": true
      },
      "security": {
        "encryption": true,
        "authentication": true,
        "authorization": true,
        "audit_logging": true,
        "threat_intelligence": true,
        "zero_trust": true,
        "data_protection": true,
        "privacy_compliance": true
      },
      "performance": {
        "real_time_processing": true,
        "machine_learning": true,
        "ai_detection": true,
        "automation": true,
        "scalability": true
      },
      "components": {
        "AdvancedSecurityDashboard": {
          "enabled": true,
          "features": {
            "threat_monitoring": true,
            "incident_management": true,
            "vulnerability_assessment": true,
            "compliance_reporting": true,
            "security_analytics": true,
            "real_time_alerts": true
          }
        },
        "SecurityDashboard": {
          "enabled": true,
          "features": {
            "basic_monitoring": true,
            "alert_management": true,
            "security_overview": true,
            "basic_analytics": true
          }
        }
      },
      "ui": {
        "theme": "auto",
        "animations": true,
        "responsive": true,
        "accessibility": true
      },
      "performance": {
        "lazyLoading": true,
        "codeSplitting": true,
        "memoization": true,
        "virtualization": true
      }
    },
    "overrides": {
      "development": {
        "features": {
          "automated_response": false,
          "penetration_testing": false
        },
        "ui": {
          "animations": false
        }
      },
      "production": {
        "features": {
          "automated_response": true,
          "penetration_testing": true
        }
      }
    }
  },
  "features": {
    "threat_detection": true,
    "incident_response": true,
    "vulnerability_scanning": true,
    "penetration_testing": true,
    "compliance_monitoring": true,
    "access_control": true,
    "security_analytics": true,
    "real_time_monitoring": true,
    "automated_response": true,
    "security_orchestration": true,
    "ui": true,
    "performance": true,
    "security": true
  },
  "ui": {
    "theme": "auto",
    "animations": true,
    "accessibility": true
  },
  "performance": {
    "lazy": true,
    "cache": true,
    "optimize": true,
    "bundle": true
  },
  "security": {
    "validate": true,
    "sanitize": true,
    "encrypt": true,
    "audit": true
  },
  "lifecycle": {
    "init": "initializeAdvancedCybersecurity",
    "destroy": "cleanupAdvancedCybersecurity"
  }
}

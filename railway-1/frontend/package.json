{
  "name": "aibos-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx next dev --turbo",
    "dev:legacy": "npx next dev",
    "dev:debug": "npx next dev --turbo --inspect",
    "build": "npx next build",
    "start": "npx next start",
    "lint": "npx next lint",
    "type-check": "tsc --noEmit",
    "clean:perf": "find . -type d -name 'node_modules' -prune -false -o -name '*.log' -o -name '*.tmp' -delete",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx}",
    "type-check:watch": "tsc --noEmit --watch",
    "analyze": "ANALYZE=true npm run build",
    "optimize": "npm run clean:perf && npm run lint:fix && npm run type-check",
    "perf:monitor": "clinic doctor -- node ./server.js",
    "perf:test": "autocannon -c 100 -d 20 http://localhost:3000",
    "clean": "if exist .next rmdir /s /q .next && if exist dist rmdir /s /q dist",
    "deps:analyze": "node ../../scripts/dependency-analyzer.js",
    "deps:flatten": "npx taze -r -w && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && npm install --omit=dev --prefer-offline --no-audit --no-fund",
    "debug:analyze": "ANALYZE=true npm run build",
    "debug:type-check": "tsc --noEmit --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "security:audit": "npm audit",
    "security:audit-prod": "npm audit --audit-level=moderate --only=prod",
    "security:fix": "npm audit fix",
    "security:monitor": "npm audit --json > security-report.json"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/supabase-js": "^2.52.1",
    "@tensorflow/tfjs-node": "^4.17.0",
    "@tensorflow/tfjs-converter": "^4.17.0",
    "@types/react-rnd": "^8.0.0",
    "@types/uuid": "^10.0.0",
    "aibos-shared-infrastructure": "^1.0.0",
    "axios": "^1.10.0",
    "canvas": "^2.11.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "framer-motion": "12.23.9",
    "lucide-react": "0.294.0",
    "next": "^14.2.30",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-rnd": "^10.5.2",
    "tailwind-merge": "^2.6.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "13.4.0",
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitest/ui": "1.0.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.0.3",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsdom": "^23.0.0",
    "postcss": "^8.5.6",
    "speed-measure-webpack-plugin": "^1.5.0",
    "tailwindcss": "3.4.17",
    "typescript": "^5.8.3",
    "vitest": "1.0.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.8.0"
  }
}

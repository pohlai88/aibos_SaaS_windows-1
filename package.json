{
  "name": "aibos-workspace",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "railway-1/frontend",
    "railway-1/backend",
    "shared"
  ],
  "scripts": {
    "dev:all": "turbo run dev",
    "build:all": "turbo run build",
    "build:prod": "turbo run build:prod",
    "build:analyze": "turbo run build:analyze",
    "clean:all": "npm run clean && npm cache clean --force",
    "lint:all": "turbo run lint",
    "type-check:all": "turbo run type-check",
    "test:all": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:integration": "turbo run test:integration",
    "test:coverage": "turbo run test:coverage",
    "test:ai:vision": "vitest run railway-1/frontend/src/ai/engines/__tests__/ComputerVisionEngine.test.ts",
    "test:ai:nlp": "vitest run railway-1/frontend/src/ai/engines/__tests__/NLPEngine.test.ts",
    "test:ai:analytics": "vitest run railway-1/frontend/src/ai/engines/__tests__/PredictiveAnalyticsEngine.test.ts",
    "test:ai:all": "npm run test:ai:vision && npm run test:ai:nlp && npm run test:ai:analytics",
    "test:cv:simple": "node scripts/test-cv-engine.js",
    "test:cv:validate": "node scripts/validate-cv-implementation.js",
    "perf:monitor": "node railway-1/frontend/server.js",
    "perf:test": "node scripts/performance-test.js",
    "deps:analyze": "node scripts/dependency-analyzer.js",
    "deps:flatten": "npx taze -r -w && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && npm install --omit=dev --prefer-offline --no-audit --no-fund",
    "debug:memory": "node --inspect=9229 --max-old-space-size=4096 railway-1/backend/dist/index.js",
    "debug:cpu": "node --inspect=9229 --cpu-prof railway-1/backend/dist/index.js",
    "debug:network": "DEBUG=aibos:*,express:* npm run dev:all",
    "debug:backend": "cd railway-1/backend && npm run dev",
    "debug:frontend": "cd railway-1/frontend && npm run dev",
    "debug:fullstack": "concurrently \"npm run debug:backend\" \"npm run debug:frontend\"",
    "debug:test": "turbo run test --parallel",
    "debug:coverage": "turbo run test:coverage",
    "debug:profile": "node railway-1/backend/dist/index.js",
    "debug:heap": "node railway-1/backend/dist/index.js",
    "debug:flame": "node railway-1/backend/dist/index.js",
    "deploy:check": "npm run type-check:all && npm run lint:all && npm run test:all",
    "deploy:build": "npm run build:prod",
    "deploy:validate": "npm run deploy:check && npm run deploy:build",
    "health:check": "curl -f http://localhost:3001/api/health || exit 1",
    "start:prod": "turbo run start:prod",
    "phase4:test": "node scripts/phase4-test-runner.js",
    "phase4:compliance": "node scripts/phase4-test-runner.js --compliance-only",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "security:fix-force": "npm audit fix --force",
    "clean": "powershell -Command \"Remove-Item -Path .turbo,.next,dist,*.log,*.tmp,test-results-*.json -Recurse -Force -ErrorAction SilentlyContinue\"",
    "clean:workspace": "npm run clean:all && powershell -Command \"Remove-Item -Path .aibos-npm-cache -Recurse -Force -ErrorAction SilentlyContinue\""
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "20.19.9",
    "@vitest/ui": "^1.0.0",
    "brotli-webpack-plugin": "1.1.0",
    "chokidar": "^3.6.0",
    "compression-webpack-plugin": "11.1.0",
    "concurrently": "8.2.2",
    "jsdom": "^23.0.0",
    "license-checker": "^25.0.1",
    "taze": "^0.8.5",
    "turbo": "2.5.5",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@10.8.0",
  "dependencies": {
    "framer-motion": "12.23.9",
    "lucide-react": "0.294.0",
    "tailwindcss": "3.4.17"
  }
}
